void setup(){
  size(800,800);
  
}

void draw(){
  background(255,255,255);
  fill(random(255),random(255),random(255));
  ellipse(mouseX+20,mouseY+20,10,10);
  fill(random(255),random(255),random(255));
  ellipse(mouseX-20,mouseY-20,10,10);
  fill(random(255),random(255),random(255));
  ellipse(mouseX+20,mouseY-20,10,10);
  fill(random(255),random(255),random(255));
  ellipse(mouseX-20,mouseY+20,10,10);
}
